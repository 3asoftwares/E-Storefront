{
  "name": "@3asoftwares/types",
  "version": "1.0.3",
  "description": "TypeScript type definitions for 3A Softwares E-Commerce",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn exec tsup src/index.ts --format cjs,esm --dts --clean",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "ecommerce",
    "types",
    "typescript",
    "microservices",
    "3asoftwares"
  ],
  "author": "3aSoftwares",
  "license": "MIT",
  "homepage": "https://github.com/3asoftwares/E-Commerce#readme",
  "bugs": {
    "url": "https://github.com/3asoftwares/E-Commerce/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3asoftwares/E-Commerce.git",
    "directory": "packages/types"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16"
  }
}
