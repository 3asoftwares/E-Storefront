{
  "name": "@3asoftwares/types",
  "version": "1.0.1",
  "description": "TypeScript type definitions for 3A Softwares E-Commerce",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --external @3asoftwares/utils",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "ecommerce",
    "types",
    "typescript",
    "microservices"
  ],
  "author": "3aSoftwares",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/3asoftwares/E-Commerce-Microservices-Platform.git",
    "directory": "packages/types"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16"
  }
}
