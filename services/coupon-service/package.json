{
  "name": "@3asoftwares/coupon-service",
  "license": "ISC",
  "version": "1.0.2",
  "description": "Coupon service with CRUD operations",
  "main": "dist/index.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "build:bundle": "tsc && ncc build dist/src/serverless.js -o vercel-output --minify",
    "build:prod": "yarn install --production=false && tsc",
    "build:vercel": "tsc && ncc build dist/src/serverless.js -o .vercel-build --minify",
    "build:render": "cd ../.. && yarn build:types && yarn build:utils && cd services/coupon-service && tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@3asoftwares/utils": "^1.0.0",
    "jsonwebtoken": "^9.0.2",
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@vercel/ncc": "^0.38.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2"
  }
}
