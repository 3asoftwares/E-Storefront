{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": null,
  "installCommand": "cd ../.. && yarn install && yarn build:types && yarn build:utils",
  "buildCommand": "yarn build:vercel",
  "outputDirectory": ".",
  "builds": [
    {
      "src": ".vercel-build/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": ".vercel-build/index.js"
    }
  ]
}
